{
  "env": {
    "browser": true,
    "node": true,
  },
  "globals": {
      "_": true,
      "$": true
  },
  "rules": {
    "camelcase": 0,
    "comma-spacing": 0,
    "quotes": [0, "single"],
    "new-cap": 0,
    "no-extend-native": 0,
    "no-multi-spaces": 0,
    "space-infix-ops": 0,
    "strict": 0
  }
}
